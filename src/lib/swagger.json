    "/auth/kakao/callback": {
      "get": {
        "tags": ["Auth"],
        "summary": "카카오 로그인",
        "description": "🌸 카카오 로그인 후 Access Token, Refresh Token과 함께 유저 정보를 반환합니다. \n\n 메인페이지에서 로그인 시에도 본 API 사용하시면 됩니다.",
        "responses": {
          "200(1)": {
            "description": "회원가입 성공",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 200
                    },
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "example": "회원가입에 성공하였습니다"
                    },
                    "data": {
                      "type": "object",
                      "example": {
                        "user": {
                          "id": 1,
                          "kakaoUid": 111281217,
                          "nickname": "얄루",
                          "phone": null,
                          "gender": "female",
                          "birthday": null,
                          "accessToken": "adja;kldhj;a.dasjlh;kadjlh;akjh;j.adjlhk;aejh",
                          "refreshToken": "adsjg;ldgjk.jdagl;kjad;lgkjad.da;gkljad;lgj"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "200(2)": {
            "description": "로그인 성공",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 200
                    },
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "example": "로그인에 성공하였습니다"
                    },
                    "data": {
                      "type": "object",
                      "example": {
                        "user": {
                          "id": 1,
                          "kakaoUid": 111281217,
                          "nickname": "얄루",
                          "phone": null,
                          "gender": "female",
                          "birthday": null,
                          "accessToken": "adja;kldhj;a.dasjlh;kadjlh;akjh;j.adjlhk;aejh",
                          "refreshToken": "adsjg;ldgjk.jdagl;kjad;lgkjad.da;gkljad;lgj"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "카카오 인가코드가 없는 경우",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Fail"
                },
                "examples": {
                  "1": {
                    "summary": "NO_CODE",
                    "value": {
                      "status": 400,
                      "success": false,
                      "message": "인가코드가 없습니다"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
