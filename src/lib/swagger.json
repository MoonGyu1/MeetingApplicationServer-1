    "/auth/refresh": {
      "post": {
        "tags": ["Auth"],
        "summary": "토큰 재발급",
        "description": "🌸 토큰을 재발급합니다. \n\n 복잡해보이지만 에러코드가 200, 400, 401인 경우로 분류하여 401인 경우 모두 새로 로그인하도록 처리해주면 됩니다! (로그인 페이지로 이동시키기)",
        "parameters": [
          {
            "in": "header",
            "name": "authorization",
            "schema": {
              "type": "string",
              "format": "Bearer {token}",
              "example": "Bearer addagkjdagjdakg.djagkjdgkadjgk.ajdkglajdlgkj"
            },
            "required": true,
            "description": "Access Token"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "properties": {
                  "grant_type": {
                    "type": "string",
                    "example": "refresh_token",
                    "description": "고정값"
                  },
                  "refresh_token": {
                    "type": "string",
                    "format": "text",
                    "example": "addagkjdagjdakg.djagkjdgkadjgk.ajdkglajdlgkj"
                  },
                  "userId": {
                    "type": "integer",
                    "example": 1
                  }
                },
                "required": ["grant_type", "refresh_token", "userId"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "토큰 재발급 성공",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 200
                    },
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "example": "토큰 재발급에 성공하였습니다"
                    },
                    "data": {
                      "type": "object",
                      "example": {
                        "user": {
                          "accessToken": "adja;kldhj;a.dasjlh;kadjlh;akjh;j.adjlhk;aejh",
                          "birthday": "",
                          "createdAt": "2022-08-15T11:13:16.000Z",
                          "gender": "female",
                          "id": 1,
                          "isDeleted": 0,
                          "kakaoUid": 111281217,
                          "nickname": "얄루",
                          "phone": null,
                          "refreshToken": "adsjg;ldgjk.jdagl;kjad;lgkjad.da;gkljad;lgj",
                          "updatedAt": "2022-08-15T11:13:16.000Z"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "유효한 토큰",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Fail"
                },
                "examples": {
                  "1": {
                    "summary": "Response Body",
                    "value": {
                      "status": 400,
                      "success": false,
                      "message": "만료되지 않은 토큰입니다"
                    }
                  }
                }
              }
            }
          },
          "401(1)": {
            "description": "토큰 없음",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Fail"
                },
                "examples": {
