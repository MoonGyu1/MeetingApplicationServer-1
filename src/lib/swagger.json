    "/auth/phone": {
      "post": {
        "tags": ["Auth"],
        "summary": "유저 핸드폰 번호 저장 및 수정",
        "description": "🌸 매칭을 신청하는 유저의 핸드폰 번호를 저장 및 수정합니다. \n\n * 카카오 로그인 후 본 API를 호출해주시면 됩니다!",
        "parameters": [
          {
            "in": "header",
            "name": "authorization",
            "schema": {
              "type": "string",
              "format": "Bearer {token}",
              "example": "Bearer addagkjdagjdakg.djagkjdgkadjgk.ajdkglajdlgkj"
            },
            "required": true,
            "description": "Access Token"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "properties": {
                  "userId": {
                    "type": "integer",
                    "example": 1
                  },
                  "phone": {
                    "type": "string",
                    "format": "varchar(13)"
                  }
                },
                "required": ["ourteamId", "phone"]
              },
              "examples": {
                "1": {
                  "value": { "userId": 1, "phone": "01012345678" }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "유저 핸드폰 번호 저장 성공",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 200
                    },
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "example": "유저 핸드폰 번호 저장에 성공하였습니다"
                    },
                    "data": {
                      "type": "object",
                      "example": {
                        "phone": "01012345678"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/auth/refresh": {
      "post": {
        "tags": ["Auth"],
        "summary": "토큰 재발급",
        "description": "🌸 토큰을 재발급합니다. \n\n 복잡해보이지만 에러코드가 200, 400, 401인 경우로 분류하여 401인 경우 모두 새로 로그인하도록 처리해주면 됩니다! (로그인 페이지로 이동시키기)",
        "parameters": [
          {
            "in": "header",
            "name": "authorization",
            "schema": {
              "type": "string",
              "format": "Bearer {token}",
              "example": "Bearer addagkjdagjdakg.djagkjdgkadjgk.ajdkglajdlgkj"
            },
            "required": true,
            "description": "Access Token"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "properties": {
                  "grant_type": {
                    "type": "string",
                    "example": "refresh_token",
                    "description": "고정값"
                  },
                  "refresh_token": {
                    "type": "string",
                    "format": "text",
                    "example": "addagkjdagjdakg.djagkjdgkadjgk.ajdkglajdlgkj"
                  },
                  "userId": {
                    "type": "integer",
                    "example": 1
                  }
                },
                "required": ["grant_type", "refresh_token", "userId"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "토큰 재발급 성공",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 200
                    },
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "example": "토큰 재발급에 성공하였습니다"
                    },
                    "data": {
                      "type": "object",
                      "example": {
                        "user": {
                          "id": 1,
                          "kakaoUid": 111281217,
                          "nickname": "얄루",
                          "phone": null,
                          "gender": "female",
                          "birthday": null,
                          "accessToken": "adja;kldhj;a.dasjlh;kadjlh;akjh;j.adjlhk;aejh",
                          "refreshToken": "adsjg;ldgjk.jdagl;kjad;lgkjad.da;gkljad;lgj"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "유효한 토큰",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Fail"
                },
                "examples": {
                  "1": {
                    "summary": "NOT_EXPIRED_TOKEN",
                    "value": {
                      "status": 400,
                      "success": false,
                      "message": "만료되지 않은 토큰입니다"
                    }
                  }
                }
              }
            }
          },
          "401(1)": {
            "description": "토큰 없음",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Fail"
                },
                "examples": {
                  "1": {
                    "summary": "TOKEN_EMPTY",
                    "value": {
                      "status": 401,
                      "success": false,
                      "message": "토큰이 없습니다"
                    }
                  }
                }
              }
            }
          },
          "401(2)": {
            "description": "허가 유형 없음",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Fail"
                },
                "examples": {
                  "1": {
                    "summary": "INVALID_GRANT_TYPE",
                    "value": {
                      "status": 401,
                      "success": false,
                      "message": "올바르지 않은 허가 유형입니다"
                    }
                  }
                }
              }
            }
          },
          "401(3)": {
            "description": "유저 아이디 없음",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Fail"
                },
                "examples": {
                  "1": {
                    "summary": "NO_USER_ID",
                    "value": {
                      "status": 401,
                      "success": false,
                      "message": "유저 아이디가 없습니다"
                    }
                  }
                }
              }
            }
          },
          "401(4)": {
            "description": "유효하지 않은 토큰",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Fail"
                },
                "examples": {
                  "1": {
                    "summary": "TOKEN_INVALID",
                    "value": {
                      "status": 401,
                      "success": false,
                      "message": "토큰이 유효하지 않습니다"
                    }
                  }
                }
              }
            }
          },
          "401(5)": {
            "description": "만료된 토큰",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Fail"
                },
                "examples": {
                  "1": {
                    "summary": "TOKEN_EXPIRED",
                    "value": {
                      "status": 401,
                      "success": false,
                      "message": "토큰이 만료되었습니다"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
