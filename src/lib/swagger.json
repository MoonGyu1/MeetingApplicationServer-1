    "/user/ourteam": {
      "post": {
        "tags": ["User"],
        "summary": "유저 정보 저장",
        "description": "🌸 매칭을 신청하는 유저의 팀 정보를 저장합니다. \n\n카카오 로그인에 성공한 후 발급되는 토큰과 함께 본 API를 호출해주시면 됩니다! \n\n에러1. 토큰이 만료된 경우 api/auth/refresh 호출하여 토큰 재발급 후 다시 호출 \n\n에러2. 토큰이 유효하지 않은 경우 재로그인 해주시면 됩니다!",
        "parameters": [
          {
            "in": "header",
            "name": "authorization",
            "schema": {
              "type": "string",
              "format": "Bearer {token}",
              "example": "Bearer addagkjdagjdakg.djagkjdgkadjgk.ajdkglajdlgkj"
            },
            "required": true,
            "description": "Access Token"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "properties": {
                  "userId": {
                    "type": "integer",
                    "example": "1"
                  },
                  "gender": {
                    "type": "string",
                    "format": "varchar(2)",
                    "example": "남성"
                  },
                  "num": {
                    "type": "string",
                    "format": "varchar(3)",
                    "example": "3:3"
                  },
                  "age": {
                    "type": "string",
                    "format": "varchar(2)",
                    "example": "25"
                  },
                  "intro": {
                    "type": "string",
                    "format": "text",
                    "example": "안녕하세요. 저희는~"
                  },
                  "job": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "minitems": 1,
                    "maxitems": 4,
                    "example": ["대학생", "취준생"]
                  },
                  "university": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "minitems": 1,
                    "maxitems": 3,
                    "example": ["경희대학교", "서울시립대학교", "다른 학교에요"]
                  },
                  "area": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "minitems": 1,
                    "maxitems": 4,
                    "example": ["홍대/합정", "강남", "건대"]
                  },
                  "day": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "minitems": 1,
                    "maxitems": 7,
                    "example": ["월요일", "토요일", "일요일"]
                  },
                  "appearance": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "minitems": 1,
                    "maxitems": 5,
                    "example": ["고양이상", "강아지상"]
                  },
                  "mbti": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "minitems": 1,
                    "maxitems": 16,
                    "example": ["ISFP", "ENTP"]
                  },
                  "fashion": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "minitems": 1,
                    "maxitems": 10,
                    "example": ["캐주얼", "비즈니스", "베이직", "아메카지"]
                  },
                  "role": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "minitems": 1,
                    "maxitems": 4,
                    "example": ["깍두기", "사회자", "비주얼"]
                  }
                },
                "required": [
                  "userId",
                  "gender",
                  "num",
                  "age",
                  "intro",
                  "job",
                  "university",
                  "area",
                  "day",
                  "appearance",
                  "mbti",
                  "fashion",
                  "role"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "유저 정보 저장 성공",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 200
                    },
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "example": "유저 정보 저장에 성공하였습니다"
                    },
                    "data": {
                      "type": "object",
                      "example": {
                        "ourteamId": 2
                      }
                    }
                  }
                }
              }
            }
          },
          "400(1)": {
            "description": "유효하지 않은 값",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Fail"
                },
                "examples": {
                  "1": {
                    "summary": "Response Body",
                    "value": {
                      "status": 400,
                      "success": false,
                      "message": "필수 항목을 모두 입력해주세요"
                    }
                  }
                }
              }
            }
          },
          "400(2)": {
            "description": "토큰 없음",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Fail"
                },
                "examples": {
                  "1": {
                    "summary": "Response Body",
                    "value": {
                      "status": 400,
                      "success": false,
                      "message": "토큰이 없습니다"
                    }
                  }
                }
              }
            }
          },
          "401(1)": {
            "description": "유효하지 않은 토큰",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Fail"
                },
                "examples": {
                  "1": {
                    "summary": "Response Body",
                    "value": {
                      "status": 401,
                      "success": false,
                      "message": "토큰이 유효하지 않습니다"
                    }
                  }
                }
              }
            }
          },
          "401(2)": {
            "description": "만료된 토큰",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Fail"
                },
                "examples": {
                  "1": {
                    "summary": "Response Body",
                    "value": {
                      "status": 401,
                      "success": false,
                      "message": "토큰이 만료되었습니다"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "존재하지 않는 유저",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Fail"
                },
                "examples": {
                  "1": {
                    "summary": "Response Body",
                    "value": {
                      "status": 404,
                      "success": false,
                      "message": "존재하지 않는 유저입니다"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/user/preference": {
      "post": {
        "tags": ["User"],
        "summary": "유저 선호 정보 저장",
        "description": "🌸 매칭을 신청하는 유저의 선호 정보를 저장합니다. \n\n ✨중요: 유저 정보 저장 API(api/user/ourteam)를 먼저 호출하신 다음, **성공 response를 받은 후(status:200)** 본 API를 호출해주세요! \n *  유저 정보 저장 API(api/user/ourteam)의 response에 담겨있는 ourteamId 값을 본 API 호출 시 함께 보내주시면 됩니다. \n * 성공 response의 data는 현재 쓰이는 곳이 없어서 임시로 success=true를 반환합니다!",
        "parameters": [
          {
            "in": "header",
            "name": "authorization",
            "schema": {
              "type": "string",
              "format": "Bearer {token}",
              "example": "Bearer addagkjdagjdakg.djagkjdgkadjgk.ajdkglajdlgkj"
            },
            "required": true,
            "description": "Access Token"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "properties": {
                  "ourteamId": {
                    "type": "integer",
                    "example": 2
                  },
                  "job": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "minitems": 1,
                    "maxitems": 4,
                    "example": ["대학생", "취준생"]
                  },
                  "age": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "minitems": 2,
                    "maxitems": 2,
                    "example": ["21", "25"]
                  },
                  "sameUniversity": {
                    "type": "integer",
                    "example": 1,
                    "description": "0: 같은 학교는 싫어요, 1: 상관 없어요"
                  },
                  "vibe": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "minitems": 1,
                    "maxitems": 7,
                    "example": ["대화가 좋아요", "재미"]
                  }
                },
                "required": ["ourteamId", "job", "age", "same", "sameUniversity", "vibe"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "유저 선호 정보 저장 성공",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 200
                    },
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "example": "유저 선호 정보 저장에 성공하였습니다"
                    },
                    "data": {
                      "type": "object",
                      "example": {
                        "success": true
                      }
                    }
                  }
                }
              }
            }
          },
